{
  "name": "@gio-design/icon",
  "version": "0.0.32",
  "description": "GrowingIO Design System Icons",
  "license": "MIT",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "yarn svg-icon && rollup -c && yarn gen-list",
    "svg-icon": "if [ ! -d dist ]; then mkdir dist; fi && yarn svg-icon-generate --svgDir assets --destination dist/Icon.js --keepFillColor true --radium false",
    "test": "echo \"Error: run tests from root\" && exit 1",
    "gen-list": "node scripts/gen-list.js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@gio-design/react-svg-icon-generator": "0.5.9",
    "rollup-plugin-babel": "^4.3.3"
  },
  "dependencies": {
    "radium": "^0.25.2"
  }
}