{
  "compilerOptions": {
    "module": "ES6",
    "removeComments": true,
    "preserveConstEnums": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "keyofStringsOnly": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["es2015", "dom"]
  }
}
